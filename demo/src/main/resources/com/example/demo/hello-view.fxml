<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefWidth="900.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.TableController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TableView fx:id="csvTable" prefWidth="860.0" />
   <TableView fx:id="table" prefWidth="860.0" />


   <Button mnemonicParsing="false" onAction="#selectFileFromExplorer" text="File Explorer" />
   <Button mnemonicParsing="false" onAction="#onSaveButton" text="Save file" />


</VBox>
